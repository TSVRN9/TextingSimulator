{
  "name": "TextingSimulator",
  "version": "1.0.0",
  "repository": "https://github.com/TSVRN9/TextingSimulator.git",
  "author": "tavern <66744142+TSVRN9@users.noreply.github.com>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "@parcel/packager-raw-url": "2.8.1",
    "@parcel/transformer-webmanifest": "2.8.1",
    "autoprefixer": "^10.4.13",
    "colors": "^1.4.0",
    "crypto-js": "^4.1.1",
    "gh-pages": "^4.0.0",
    "parcel": "^2.8.1",
    "parcel-namer-hashless": "^1.0.3",
    "postcss": "^8.4.20",
    "tailwindcss": "^3.2.4",
    "workbox-cli": "^6.5.4"
  },
  "scripts": {
    "start": "yarn encrypt && parcel serve src/index.html",
    "build": "parcel build src/index.html --public-url . --no-content-hash && yarn post-build",
    "post-build": "node encrypt.js && workbox generateSW",
    "deploy": "gh-pages -d dist",
    "encrypt": "node encrypt.js",
    "clean": "rm -rf dist && rm -rf .parcel-cache"
  },
  "cache": {
    "disablePlugin": false,
    "inDev": true,
    "strategy": "default",
    "importWorkboxFrom": "local"
  }
}
